{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "modelVersion": {
            "type": "string",
            "pattern": "^[0-9]+(\\.[0-9]+)*$"
        },
        "parameters": {
            "type": "object",
            "additionalProperties": {
                "type": "string"
            }
        }
    },
    "additionalProperties": false,
    "required": ["modelVersion", "parameters"]
}